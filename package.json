{
	"name": "with-typescript",
	"version": "1.0.0",
	"scripts": {
		"dev": "next",
		"build": "next build",
		"start": "next start",
		"type-check": "tsc",
		"migrate-save": "prisma migrate save --experimental",
		"migrate-up": "prisma migrate up --experimental",
		"generate": "npm run generate-prisma && npm -s run generate:prisma && npm -s run generate:nexus",
		"generate-prisma": "prisma introspect",
		"generate:prisma": "prisma generate",
		"generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql",
		"relay": "relay-compiler --src ./src --schema ./pages/api/schema.graphql --extensions js jsx"
	},
	"dependencies": {
		"@apollo/client": "^3.3.4",
		"@nexus/schema": "^0.19.2",
		"@prisma/cli": "^2.12.1",
		"@prisma/client": "^2.13.0",
		"@prisma/nexus": "0.0.1",
		"@types/axios": "^0.14.0",
		"@types/next": "^9.0.0",
		"apollo-server": "^2.19.0",
		"apollo-server-micro": "^2.19.0",
		"axios": "^0.21.0",
		"graphql": "^15.4.0",
		"jsonwebtoken": "^8.5.1",
		"next": "^10.0.3",
		"nexus": "^0.30.1",
		"nexus-plugin-prisma": "^0.26.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-relay": "^10.1.0",
		"styled-components": "^5.2.1",
		"ts-node": "^9.0.0"
	},
	"devDependencies": {
		"@types/jsonwebtoken": "^8.5.0",
		"@types/node": "^12.12.21",
		"@types/react": "^16.9.16",
		"@types/react-dom": "^16.9.4",
		"@types/react-relay": "^7.0.17",
		"@types/styled-components": "^5.1.5",
		"@zeit/next-typescript": "^1.1.1",
		"babel-plugin-styled-components": "^1.12.0",
		"relay-compiler": "^10.1.1",
		"relay-config": "^10.1.1",
		"typescript": "4.0"
	},
	"license": "MIT"
}
